# Environment variables for docker-compose
# Copy this file to .env and update values

# MySQL Configuration
MYSQL_ROOT_PASSWORD=root123
MYSQL_DATABASE=vehiclebooking
MYSQL_USER=dbuser
MYSQL_PASSWORD=dbpass123

# Application Configuration
LOG_LEVEL=DEBUG
SHOW_SQL=false

# JWT Configuration
JWT_SECRET=mySecretKeyForJWTTokenGenerationChangeThisInProduction
JWT_EXPIRATION=86400000

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:19006,http://localhost:8081

# Firebase Configuration Path
GOOGLE_APPLICATION_CREDENTIALS=/app/firebase-credentials.json
